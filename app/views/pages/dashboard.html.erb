<div class="container">
  <div class="row">
    <div class="col-8 p-5">

      <h2><%= current_user.name %>'s DashBoard </h2>
        <h4>Card Center</h4>
          <div class="container">

            <% @credit_cards = CreditCard.all %>
            <% @credit_cards.each do |credit_card| %>
                <p>Card ID: <%= credit_card.id %> | <%= credit_card.card_number %></p>
            <% end %>
          </div>

          <div class="container">
            <h4>Saving Balance</h4>
            <% @transction = Transaction.all %>
            <% @transction.each do |tsn| %>
              <p><%= tsn.transaction_amount %> | <%= next_value(tsn.transaction_amount) %></p>
            <% end %>
          </div>




    </div>
  </div>
</div>
